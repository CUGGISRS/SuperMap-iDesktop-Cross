<?xml version="1.0" encoding="utf-8" standalone="no"?>
<plugin xmlns="http://www.supermap.com.cn/desktop" author="SuperMap" description="WorkflowView Plugin"
        helpLocalRoot="..\Help\WebHelp\" helpOnlineRoot="http://support.supermap.com.cn/onlinedoc/deskpronet/"
        name="SuperMap.Desktop.WorkflowView" url="www.supermap.com.cn">
    <runtime bundleName="./Plugins/WorkflowView/SuperMap.Desktop.WorkflowView.dll" enabled="True"/>
    <frameMenus>
        <frameMenu formClass="" id="View" index="4" label="视图" visible="true">
            <group id="Browser" index="1" label="浏览" visible="true">
                <button checkState="false" customProperty="" helpURL=""
                        image="../Resources/Process/Menu/NewWorkflow.png" index="3" label="新建工作流"
                        onAction="CtrlActionNewWorkflow" screenTip="新建一个场景窗口。" screenTipImage="" shortcutKey=""
                        visible="true"/>
            </group>
        </frameMenu>
    </frameMenus>
    <toolbars>
        <toolbar formClass="" id="Process" index="2" label="Process" rowIndex="0" visible="true">
            <button checkState="false" customProperty="" helpURL="" image="../Resources/Process/ToolBar/Process.png"
                    index="1" label="新建工作流" onAction="CtrlActionNewWorkflow" screenTip="" screenTipImage=""
                    shortcutKey="" visible="true"/>
            <button checkState="false" customProperty="" helpURL="" image="../Resources/Process/ToolBar/import.png"
                    index="5" label="导入模板" onAction="CtrlActionImportWorkflow" screenTip="" screenTipImage=""
                    shortcutKey="" visible="true"/>
            <!--<separator index="8"/>-->
            <!--<button checkState="false" customProperty="" helpURL="" image="../Resources/Process/ToolBar/PointLight.png" index="9" label="演示视频" onAction="CtrlActionDEMO" screenTip="用户大会展台演示" screenTipImage="" shortcutKey="" visible="true"/>-->
        </toolbar>
        <toolbar formClass="SuperMap.Desktop.FormWorkflow" id="Workflow" index="3" label="Process" rowIndex="0"
                 visible="true">
            <button checkState="false" customProperty="" helpURL="" image="../Resources/Process/ToolBar/Connect.png"
                    index="2" label="连接" onAction="CtrlActionConnect" screenTip="" screenTipImage="" shortcutKey=""
                    visible="true"/>
            <button checkState="false" customProperty="" helpURL="" image="../Resources/Process/ToolBar/Run.png"
                    index="3" label="执行" onAction="CtrlActionRun" screenTip="" screenTipImage="" shortcutKey=""
                    visible="true"/>
            <button checkState="false" customProperty="" helpURL="" image="../Resources/Process/ToolBar/export.png"
                    index="4" label="导出模板" onAction="CtrlActionExportWorkflow" screenTip="" screenTipImage=""
                    shortcutKey="" visible="true"/>
        </toolbar>
        <separator index="8"/>
        <button checkState="false" customProperty="" helpURL="" image="../Resources/Process/ToolBar/PointLight.png"
                index="9" label="演示视频" onAction="CtrlActionDEMO" screenTip="用户大会展台演示" screenTipImage="" shortcutKey=""
                visible="true"/>
    </toolbars>
    <dockbars>
        <dockbar component="com.supermap.desktop.WorkflowView.ProcessManagerPanel" dockDirection="leftBottom"
                 id="ProcessManager" title="工具箱" visible="false"/>
        <dockbar component="com.supermap.desktop.WorkflowView.ParameterManager" dockDirection="rightTop"
                 id="ParameterManager" title="参数设置" visible="false"/>
        <dockbar component="com.supermap.desktop.WorkflowView.tasks.TasksManagerContainer" dockDirection="right"
                 id="TasksManagerContainer" title="任务管理" visible="false"/>
    </dockbars>
    <contextMenus>
        <contextMenu id="Supermap.Desktop.UI.WorkspaceControlManager.ContextMenuWorkFlows" index="30"
                     label="工作空间管理器_工作流集合">
            <group id="WorkFlows" index="0" label="工作流集合" visible="true">
                <button checkState="false" image="../Resources/Process/NewWorkFlow.png" index="0" label="新建工作流"
                        onAction="CtrlActionWorkflow" visible="true"/>
                <button checkState="false" image="" index="1" label="加载工作流模板..." onAction="CtrlActionProcessImport"
                        visible="true"/>
            </group>
        </contextMenu>
        <contextMenu id="Supermap.Desktop.UI.WorkspaceControlManager.ContextMenuWorkFlow" index="31"
                     label="工作空间管理器_工作流">
            <group id="WorkFlow" index="0" label="工作流" visible="true">
                <button checkState="false" image="" index="0" label="打开工作流" onAction="CtrlActionOpenWorkFlow"
                        visible="true"/>
                <button checkState="false" image="" index="1" label="工作流另存为..." onAction="CtrlActionTreeWorkFlowSaveAs"
                        visible="true"/>
                <button checkState="false" image="" index="2" label="输出工作流模板..." onAction="CtrlActionProcessExportTree"
                        visible="true"/>
            </group>
            <group id="WorkFlowOperation" index="1" label="工作流操作" visible="true">
                <button checkState="false" image="../Resources/Core/Menu/Delete.png" index="0" label="删除工作流"
                        onAction="CtrlActionProcessDelete" visible="true"/>
                <button checkState="false" image="../Resources/Core/Menu/Rename.png" index="1" label="重命名"
                        onAction="CtrlActionProcessRename" visible="true"/>
            </group>
        </contextMenu>
        <contextMenu id="SuperMap.Desktop.FormWorkflow.FormContextMenu" index="30" label="工作流窗口_工作流">
            <group id="FullWind" index="4" label="整窗显示" visible="true">
                <button checkState="false" image="" index="0" label="全幅显示" onAction="CtrlActionEntireView" screenTip=""
                        screenTipImage="" shortcutKey="" visible="true"/>
            </group>
            <group id="Process" index="0" label="工作流窗口" visible="true">
                <button checkState="false" image="" index="0" label="保存工作流" onAction="CtrlActionSave"/>
                <button checkState="false" image="" index="1" label="工作流另存为..." onAction="CtrlActionProcessSaveAs"/>
                <button checkState="false" image="../Resources/Process/ToolBar/export.png" index="2" label="输出工作流模板..."
                        onAction="CtrlActionExportWorkflow" visible="true"/>
            </group>
            <group id="Settings" index="5" label="设置" visible="true">
                <button checkState="false" image="" index="0" label="参数设置" onAction="CtrlActionParameters"/>
            </group>
            <group id="Settings" index="6" label="任务管理" visible="true">
                <button checkState="false" image="" index="0" label="任务管理" onAction="CtrlActionTasksManager"/>
            </group>
        </contextMenu>
        <contextMenu id="SuperMap.Desktop.FormWorkflow.ProcessContextMenu" index="30" label="工作流窗口_功能节点">
            <group id="Process" index="0" label="工作流窗口" visible="true">
                <button checkState="false" image="" index="0" label="参数设置" onAction="CtrlActionParameters"/>
            </group>
            <group id="FullWind" index="4" label="整窗显示" visible="true">
                <button checkState="false" image="" index="0" label="全幅显示" onAction="CtrlActionEntireView" screenTip=""
                        screenTipImage="" shortcutKey="" visible="true"/>
            </group>
        </contextMenu>
        <contextMenu id="SuperMap.Desktop.FormWorkflow.OutputContextMenu" index="30" label="工作流窗口_输出节点">
            <group id="FullWind" index="4" label="整窗显示" visible="true">
                <button checkState="false" image="" index="0" label="全幅显示" onAction="CtrlActionEntireView" screenTip=""
                        screenTipImage="" shortcutKey="" visible="true"/>
            </group>
        </contextMenu>
        <contextMenu id="SuperMap.Desktop.FormWorkflow.GraphsContextMenu" index="30" label="工作流窗口_混合节点">
            <group id="FullWind" index="4" label="整窗显示" visible="true">
                <button checkState="false" image="" index="0" label="全幅显示" onAction="CtrlActionEntireView" screenTip=""
                        screenTipImage="" shortcutKey="" visible="true"/>
            </group>
        </contextMenu>
    </contextMenus>
    <statusbars>
    </statusbars>
    <ProcessManager>
        <ProcessGroup id="DataImport" index="0" title="数据导入">
            <ProcessGroup id="AutoCAD" title="AutoCAD交换格式">
                <Process descriptorClass="com.supermap.desktop.WorkflowView.meta.loader.ImportProcessDescriptor"
                         index="0" loaderClass="com.supermap.desktop.WorkflowView.meta.loader.ImportProcessLoader">
                    <ClassName>com.supermap.desktop.WorkflowView.meta.metaProcessImplements.MetaProcessImport
                    </ClassName>
                    <Title>导入.DXF</Title>
                    <Key>ImportDXF</Key>
                    <ImportType>DXF</ImportType>
                </Process>
                <Process descriptorClass="com.supermap.desktop.WorkflowView.meta.loader.ImportProcessDescriptor"
                         loaderClass="com.supermap.desktop.WorkflowView.meta.loader.ImportProcessLoader">
                    <ClassName>com.supermap.desktop.WorkflowView.meta.metaProcessImplements.MetaProcessImport
                    </ClassName>
                    <Title>导入 .DWG</Title>
                    <Key>ImportDWG</Key>
                    <ImportType>DWG</ImportType>
                </Process>
            </ProcessGroup>
        </ProcessGroup>

        <ProcessGroup id="SpatialStatisticalAnalysis" index="11" title="地统计分析">
            <ProcessGroup id="SpatialMeasure" title="度量地理分布">
                <Process>
                    <ClassName>com.supermap.desktop.WorkflowView.meta.metaProcessImplements.spatialStatistics.MetaProcessCentralElement
                    </ClassName>
                    <Title>中心要素</Title>
                    <Key>CentralElement</Key>
                </Process>
                <Process>
                    <ClassName>com.supermap.desktop.WorkflowView.meta.metaProcessImplements.spatialStatistics.MetaProcessMeanCenter
                    </ClassName>
                    <Title>平均中心</Title>
                    <Key>MeanCenter</Key>
                </Process>
                <Process>
                    <ClassName>
                        com.supermap.desktop.WorkflowView.meta.metaProcessImplements.spatialStatistics.MetaProcessMedianCenter
                    </ClassName>
                    <Title>中位数中心</Title>
                    <Key>MedianCenter</Key>
                </Process>
                <Process>
                    <ClassName>
                        com.supermap.desktop.WorkflowView.meta.metaProcessImplements.spatialStatistics.MetaProcessDirectional
                    </ClassName>
                    <Title>方向分布</Title>
                    <Key>Directional</Key>
                </Process>
                <Process>
                    <ClassName>
                        com.supermap.desktop.WorkflowView.meta.metaProcessImplements.spatialStatistics.MetaProcessLinearDirectionalMean
                    </ClassName>
                    <Title>线性方向平均值</Title>
                    <Key>LinearDirectionalMean</Key>
                </Process>
                <Process>
                    <ClassName>
                        com.supermap.desktop.WorkflowView.meta.metaProcessImplements.spatialStatistics.MetaProcessStandardDistance
                    </ClassName>
                    <Title>标准距离</Title>
                    <Key>StandardDistance</Key>
                </Process>
            </ProcessGroup>
            <ProcessGroup id="AnalyzingPatterns" title="分析模式">
                <Process>
                    <ClassName>
                        com.supermap.desktop.WorkflowView.meta.metaProcessImplements.spatialStatistics.MetaProcessAutoCorrelation
                    </ClassName>
                    <Title>空间自相关</Title>
                    <Key>AutoCorrelation</Key>
                </Process>
                <Process>
                    <ClassName>
                        com.supermap.desktop.WorkflowView.meta.metaProcessImplements.spatialStatistics.MetaProcessHighOrLowClustering
                    </ClassName>
                    <Title>高低值聚类</Title>
                    <Key>HighOrLowClustering</Key>
                </Process>
                <Process>
                    <ClassName>
                        com.supermap.desktop.WorkflowView.meta.metaProcessImplements.spatialStatistics.MetaProcessIncrementalAutoCorrelation
                    </ClassName>
                    <Title>增量空间自相关</Title>
                    <Key>IncrementalAutoCorrelation</Key>
                </Process>
                <Process>
                    <ClassName>
                        com.supermap.desktop.WorkflowView.meta.metaProcessImplements.spatialStatistics.MetaProcessAverageNearestNeighbor
                    </ClassName>
                    <Title>平均最近邻</Title>
                    <Key>AverageNearestNeighbor</Key>
                </Process>
            </ProcessGroup>
            <ProcessGroup id="ClusteringDistributions" title="聚类分布">
                <Process>
                    <ClassName>
                        com.supermap.desktop.WorkflowView.meta.metaProcessImplements.spatialStatistics.MetaProcessClusterOutlierAnalyst
                    </ClassName>
                    <Title>聚类和异常值分析</Title>
                    <Key>ClusterOutlierAnalyst</Key>
                </Process>
                <Process>
                    <ClassName>
                        com.supermap.desktop.WorkflowView.meta.metaProcessImplements.spatialStatistics.MetaProcessHotSpotAnalyst
                    </ClassName>
                    <Title>热点分析</Title>
                    <Key>HotSpotAnalyst</Key>
                </Process>
            </ProcessGroup>
            <ProcessGroup id="SpatialRelModeling" title="空间关系建模">
                <Process>
                    <ClassName>
                        com.supermap.desktop.WorkflowView.meta.metaProcessImplements.spatialStatistics.MetaProcessGeographicWeightedRegression
                    </ClassName>
                    <Title>地理加权回归分析</Title>
                    <Key>GeographicWeightedRegression</Key>
                </Process>
            </ProcessGroup>
        </ProcessGroup>

        <ProcessGroup id="DataRun" index="12" title="数据处理">
            <Process>
                <ClassName>
                    com.supermap.desktop.WorkflowView.meta.metaProcessImplements.dataRun.MetaProcessDissolve
                </ClassName>
                <Title>融合</Title>
                <Key>Dissolve</Key>
            </Process>
            <Process>
                <ClassName>
                    com.supermap.desktop.WorkflowView.meta.metaProcessImplements.dataRun.MetaProcessAggregatePoints
                </ClassName>
                <Title>点密度聚合</Title>
                <Key>AggregatePoints</Key>
            </Process>
            <Process>
                <ClassName>
                    com.supermap.desktop.WorkflowView.meta.metaProcessImplements.dataRun.MetaProcessRarefyPoints
                </ClassName>
                <Title>点抽稀</Title>
                <Key>RarefyPoints</Key>
            </Process>
            <Process>
                <ClassName>
                    com.supermap.desktop.WorkflowView.meta.metaProcessImplements.dataRun.MetaProcessVectorResample
                </ClassName>
                <Title>矢量重采样</Title>
                <Key>VectorResample</Key>
            </Process>
            <Process>
                <ClassName>
                    com.supermap.desktop.WorkflowView.meta.metaProcessImplements.dataRun.MetaProcessLinePolygonSmooth
                </ClassName>
                <Title>线面光滑</Title>
                <Key>LinePolygonSmooth</Key>
            </Process>
            <Process>
                <ClassName>
                    com.supermap.desktop.WorkflowView.meta.metaProcessImplements.dataRun.MetaProcessEdgeMatch
                </ClassName>
                <Title>图幅接边</Title>
                <Key>EdgeMatch</Key>
            </Process>
            <Process>
                <ClassName>
                    com.supermap.desktop.WorkflowView.meta.metaProcessImplements.dataRun.MetaProcessPickupBorder
                </ClassName>
                <Title>提取边界线</Title>
                <Key>PickupBorder</Key>
            </Process>
            <Process>
                <ClassName>
                    com.supermap.desktop.WorkflowView.meta.metaProcessImplements.dataRun.MetaProcessDualLineToCenterLine
                </ClassName>
                <Title>双线中心线</Title>
                <Key>DualLineToCenterLine</Key>
            </Process>
            <Process>
                <ClassName>
                    com.supermap.desktop.WorkflowView.meta.metaProcessImplements.dataRun.MetaProcessRegionToCenterLine
                </ClassName>
                <Title>面中心线</Title>
                <Key>RegionToCenterLine</Key>
            </Process>
            <Process>
                <ClassName>
                    com.supermap.desktop.WorkflowView.meta.metaProcessImplements.dataRun.MetaProcessRegionTrunkToCenterLine
                </ClassName>
                <Title>面主干中心线</Title>
                <Key>RegionTrunkToCenterLine</Key>
            </Process>
            <Process>
                <ClassName>
                    com.supermap.desktop.WorkflowView.meta.metaProcessImplements.dataRun.MetaProcessAppendFields
                </ClassName>
                <Title>数据集追加列</Title>
                <Key>AppendFields</Key>
            </Process>
            <Process>
                <ClassName>
                    com.supermap.desktop.WorkflowView.meta.metaProcessImplements.dataRun.MetaprocessAppendRow
                </ClassName>
                <Title>数据集追加行</Title>
                <Key>AppendRows</Key>
            </Process>
            <Process>
                <ClassName>
                    com.supermap.desktop.WorkflowView.meta.metaProcessImplements.dataRun.MetaProcessEliminate
                </ClassName>
                <Title>碎面多边形合并</Title>
                <Key>Eliminate</Key>
            </Process>
        </ProcessGroup>

        <Process>
            <ClassName>com.supermap.desktop.WorkflowView.meta.metaProcessImplements.MetaProcessBuffer</ClassName>
            <Title>缓冲区分析</Title>
            <Key>BufferAnalyst</Key>
        </Process>
    </ProcessManager>
</plugin>
